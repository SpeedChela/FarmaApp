ndex="4" value="132"/><column index="5" value="300"/><column index="6" value="226"/><column index="7" value="179"/><column index="8" value="93"/><column index="9" value="91"/><column index="10" value="81"/><column index="11" value="78"/><column index="12" value="38"/><column index="13" value="66"/><column index="14" value="69"/><column index="15" value="42"/><column index="16" value="75"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">INSERT INTO Productos (cod_barras, nom_com, descripcion, contenido, gramaje, presentacion, farmaceutica, tipo, precio_compra, precio_venta, fecha_cad, stock, min_stock, max_stock, activo, porcentajeV)
SELECT 
    TRIM(fo.codigo_barras) AS cod_barras, -- Normalizaci√≥n de la clave primaria para prevenir inconsistencias
    fo.nombre_comercial AS nom_com, 
    fo.descripcion_activo AS descripcion, 
    fo.contenido, 
    fo.gramaje, 
    fo.presentacion, 
    fo.farmaceutica, 
    NULL AS tipo, -- Campo no existente en la tabla origen, asignamos NULL
    COALESCE(NULL, 0.0) AS precio_compra, -- Valor predeterminado para precio_compra
    COALESCE(fo.precio, 0.0) AS precio_venta, -- Si precio es NULL en la tabla origen, asignamos 0.0
    fo.fecha_caducidad AS fecha_cad, 
    fo.stock, 
    fo.minimoStock AS min_stock, 
    fo.maximoStock AS max_stock, 
    fo.activo, 
    COALESCE(NULL, 0.0) AS porcentajeV -- Si porcentajeV no existe en la tabla origen, asignamos 0.0
FROM FarmaciaOLD.Productos fo
LEFT JOIN Productos p ON TRIM(fo.codigo_barras) = TRIM(p.cod_barras)
WHERE p.cod_barras IS NULL; -- Solo insertamos registros que NO existen en la tabla destino</sql><current_tab id="0"/></tab_sql></sqlb_project>
